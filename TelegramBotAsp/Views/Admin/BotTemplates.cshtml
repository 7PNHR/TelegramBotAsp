@model TelegramBotAsp.Models.Admin.BotTemplateModel

@{
    Layout = null;
}

<!DOCTYPE html>
<head>
    <title>Шаблоны</title>
</head>
<body>
<div>
    @foreach (var e in Model.Templates)
    {
        <p>Вопрос: @e.Request <br/> Ответ:@e.Response <br/> Тема:@e.TopicName</p>
        <hr/>
    }
</div>
</body>